{
  "headers": [
    {
      "source": "/(.*\\.tsx)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/babel; charset=utf-8"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
